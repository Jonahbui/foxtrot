;   param=value ; assign values to parameters

config_version=4

[application]

config/name="foxtrot"
run/main_scene="res://Scenes/SplashScreen/splash_screen.tscn"
config/icon="res://icon.png"

[audio]

default_bus_layout="res://Audio/default_bus_layout.tres"

[autoload]

Save="*res://Scripts/save.gd"

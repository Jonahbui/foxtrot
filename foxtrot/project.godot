"Enemy": ""
}

[application]

config/name="foxtrot"
run/main_scene="res://Scenes/SplashScreen/SplashScreen.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[audio]

default_bus_layout="res://Audio/default_bus_layout.tres"

[autoload]

Signals="*res://Scripts/signals.gd"
Save="*res://Scripts/save.gd"
Globals="*res://Scripts/globals.gd"
Equips="*res://Scripts/Inventory/equips.gd"
Helper="*res://Scripts/helper.gd"
Audio="*res://Scripts/audio.gd"

[display]

window/size/width=640
window/size/height=360
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="2d"
window/stretch/aspect="expand"

[global]

aspect=false
filer=false
filter=false

